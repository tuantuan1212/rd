local player = game.Players.LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")
local tweenService = game:GetService("TweenService")

local function SetTeam(teamName)
    replicatedStorage.Remotes.CommF_:InvokeServer("SetTeam", teamName)
end

local function TravelZou()
    replicatedStorage.Remotes.CommF_:InvokeServer("TravelZou")
end

local function SetSpawnPoint()
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer("SetSpawnPoint")
end

local function RandomAction()
    replicatedStorage.Remotes.CommF_:InvokeServer("Cousin", "Buy")
end

local function DropMultipleItemsFromBackpack(quantity)
    for _, tool in ipairs(player.Backpack:GetChildren()) do
        if tool:IsA("Tool") then
            tool.Parent = player.Character
            local toolInCharacter = player.Character:FindFirstChild(tool.Name)
            if toolInCharacter then
                toolInCharacter.Parent = game.Workspace
            end
        end
    end
end

local function UnequipTools()
    for _, tool in ipairs(player.Character:GetChildren()) do
        if tool:IsA("Tool") then
            tool.Parent = player.Backpack
        end
    end
end

local function movePlayer(destination, duration)
    local humanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        local initialPosition = humanoidRootPart.CFrame
        local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
        local tween = tweenService:Create(humanoidRootPart, tweenInfo, {CFrame = destination})
        tween:Play()
        tween.Completed:Wait()
    else
        warn("HumanoidRootPart not found. Character may not be loaded.")
    end
end

SetTeam("Pirates")
TravelZou()
wait(2)
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(-12550.5615, 338.723907, -7490.50586))
SetSpawnPoint()
wait(1)
local endPosition = CFrame.new(-12544.0811, 460.95871, -7572.50195, -0.0122566521, 0.126096845, -0.991942286, 0.00503664091, 0.992012024, 0.126043469, 0.999912143, -0.00345116667, -0.0127938986)

while true do
    wait(0)
    local success, errorMessage = pcall(function()
        movePlayer(endPosition, 0)
    end)
    wait(1)
    RandomAction()
    wait(1)
    UnequipTools()
    wait(1)
    DropMultipleItemsFromBackpack(200)
    wait(1)

    if not success then
        warn("Error during movePlayer:", errorMessage)
    end

    wait(0)
end
